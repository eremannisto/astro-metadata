---
export type LanguageAlternate = {
  href     : string
  hreflang : string
}

export type Props = {
  alternates: LanguageAlternate[]
}

const { alternates } = Astro.props
---

{alternates.map((alternate) => (
  <link
    rel="alternate"
    href={alternate.href}
    hreflang={alternate.hreflang}
  />
))}